{
  "name": "@prezly/content-format",
  "version": "0.54.0",
  "description": "Prezly Content Format types and utilities",
  "license": "MIT",
  "main": "build/commonjs/index.cjs",
  "module": "build/esm/index.mjs",
  "types": "build/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/esm/index.mjs",
      "default": "./build/commonjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prezly/content-format-js.git"
  },
  "bugs": {
    "url": "https://github.com/prezly/content-format-js/issues"
  },
  "homepage": "https://github.com/prezly/content-format-js#readme",
  "scripts": {
    "build": "npm run build:types && npm run build:cjs && npm run build:esm",
    "build:types": "rimraf build/types && tsc --project .",
    "build:cjs": "rimraf build/commonjs && babel --extensions='.ts' src/ --config-file=../../babel.cjs.config.json --out-dir=./build/commonjs && renamer --force --find '.js' --replace '.cjs' 'build/commonjs/**/*.{js,js.map}'",
    "build:esm": "rimraf build/esm && babel --extensions='.ts' src/ --config-file=../../babel.esm.config.json --out-dir=./build/esm && renamer --force --find '.js' --replace '.mjs' 'build/esm/**/*.{js,js.map}'",
    "test": "npm run test:build",
    "test:build": "npm run test:cjs && npm run test:esm && npm run test:types",
    "test:esm": "node build/esm/index.mjs",
    "test:cjs": "node build/commonjs/index.cjs",
    "test:types": "tsc --noEmit --esModuleInterop --skipLibCheck build/types/index.d.ts",
    "check": "npm run prettier",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "clean:build": "rimraf build/ .turbo/",
    "clean:node_modules": "rimraf node_modules/",
    "prettier": "prettier --check 'src/**/*.(js|ts|scss)'",
    "prettier:fix": "npm run prettier -- --write"
  },
  "devEngines": {
    "node": ">=14.0",
    "npm": ">=7.0"
  },
  "dependencies": {
    "@prezly/uploads": "^0.2.0",
    "is-plain-object": "^5.0.0"
  }
}
