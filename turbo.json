{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build:all": {
      "dependsOn": ["build:types", "build:esm", "build:cjs"]
    },
    "build:types": {
      "dependsOn": ["^build:types"],
      "outputs": ["build/types/**"]
    },
    "build:esm": {
      "dependsOn": ["^build:esm"],
      "outputs": ["build/esm/**"]
    },
    "build:cjs": {
      "dependsOn": ["^build:cjs"],
      "outputs": ["build/commonjs/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "prettier:fix": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "clean:build": {
      "outputs": []
    },
    "clean:node_modules": {
      "outputs": []
    }
  }
}