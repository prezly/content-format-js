{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build:all": {
      "dependsOn": ["build:ts", "build:esm", "build:commonjs"]
    },
    "build:ts": {
      "dependsOn": ["^build:ts"],
      "outputs": ["build/**", "dist/types/**"]
    },
    "build:esm": {
      "dependsOn": ["build:ts"],
      "outputs": ["dist/esm/**"]
    },
    "build:commonjs": {
      "dependsOn": ["build:ts"],
      "outputs": ["dist/commonjs/**"]
    },
    "test": {
      "dependsOn": ["build:ts", "build:esm", "build:commonjs"],
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "prettier:fix": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "clean:build": {
      "outputs": []
    },
    "clean:node_modules": {
      "outputs": []
    }
  }
}