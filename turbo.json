{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "build/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "prettier:fix": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "clean:build": {
      "outputs": []
    },
    "clean:node_modules": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}