{
    "name": "@prezly/content-format",
    "version": "0.51.0",
    "description": "Prezly Content Format types and utilities",
    "license": "MIT",
    "main": "build/commonjs/index.cjs",
    "module": "build/esm/index.mjs",
    "types": "build/types/index.d.ts",
    "exports": {
        "./EmailDocument": {
            "types": "./build/types/EmailDocument.d.ts",
            "import": "./build/esm/EmailDocument.mjs",
            "default": "./build/commonjs/EmailDocument.cjs"
        },
        "./NoteDocument": {
            "types": "./build/types/NoteDocument.d.ts",
            "import": "./build/esm/NoteDocument.mjs",
            "default": "./build/commonjs/NoteDocument.cjs"
        },
        "./StoryDocument": {
            "types": "./build/types/StoryDocument.d.ts",
            "import": "./build/esm/StoryDocument.mjs",
            "default": "./build/commonjs/StoryDocument.cjs"
        },
        ".": {
            "types": "./build/types/index.d.ts",
            "import": "./build/esm/index.mjs",
            "default": "./build/commonjs/index.cjs"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prezly/content-format-js.git"
    },
    "bugs": {
        "url": "https://github.com/prezly/content-format-js/issues"
    },
    "homepage": "https://github.com/prezly/content-format-js#readme",
    "scripts": {
        "build": "npm run build:types && npm run build:cjs && npm run build:esm",
        "build:types": "tsc --project .",
        "build:cjs": "babel --extensions='.ts' src/ --config-file=./babel.cjs.config.json --out-dir=./build/commonjs && renamer --force --find '.js' --replace '.cjs' 'build/commonjs/**/*.{js,js.map}'",
        "build:esm": "babel --extensions='.ts' src/ --config-file=./babel.esm.config.json --out-dir=./build/esm && renamer --force --find '.js' --replace '.mjs' 'build/esm/**/*.{js,js.map}'",
        "test": "npm run test:build",
        "test:build": "npm run test:cjs && npm run test:esm",
        "test:esm": "node build/esm/index.mjs",
        "test:cjs": "node build/commonjs/index.cjs",
        "check": "npm run typescript && npm run prettier",
        "clean": "npm run clean:build && npm run clean:node_modules",
        "clean:build": "rimraf build/",
        "clean:node_modules": "rimraf node_modules/",
        "typescript": "tsc --project .",
        "prettier": "prettier --check 'src/**/*.(js|ts|scss)'",
        "prettier:fix": "npm run prettier -- --write",
        "release": "npm run release:prepare && npm run release:publish",
        "release:prepare": "npm run clean && npm install && npm run check && npm run build",
        "release:publish": "np --no-2fa"
    },
    "devEngines": {
        "node": ">=14.0",
        "npm": ">=7.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-typescript": "^7.16.7",
        "babel-plugin-add-import-extension": "^1.6.0",
        "np": "^7.5.0",
        "prettier": "^2.6.2",
        "renamer": "^4.0.0",
        "rimraf": "^3.0.2",
        "typescript": "~4.5.0"
    },
    "dependencies": {
        "@prezly/uploads": "^0.2.0",
        "is-plain-object": "^5.0.0"
    }
}
