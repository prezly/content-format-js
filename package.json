{
    "name": "@prezly/content-format",
    "version": "0.51.0",
    "description": "Prezly Content Format types and utilities",
    "license": "MIT",
    "workspaces": [
        "packages/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prezly/content-format-js.git"
    },
    "bugs": {
        "url": "https://github.com/prezly/content-format-js/issues"
    },
    "homepage": "https://github.com/prezly/content-format-js#readme",
    "scripts": {
        "build": "npm run build --workspaces",
        "test": "npm run test --workspaces",
        "clean": "npm run clean --workspaces",
        "clean:build": "npm run clean:build --workspaces",
        "clean:node_modules": "npm run clean:node_modules --workspaces",
        "prettier": "npm run prettier --workspaces",
        "prettier:fix": "npm run prettier:fix --workspaces",
        "release": "npm run release:prepare && npm run release:publish",
        "release:prepare": "npm run clean && npm install && npm run check && npm run build",
        "release:publish": "np --no-2fa"
    },
    "devEngines": {
        "node": ">=14.0",
        "npm": ">=7.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-typescript": "^7.16.7",
        "babel-plugin-add-import-extension": "^1.6.0",
        "np": "^7.5.0",
        "prettier": "^2.6.2",
        "renamer": "^4.0.0",
        "rimraf": "^3.0.2",
        "typescript": "~4.5.0"
    }
}
